<script src="~/lib/momentjs/moment.min.js" asp-append-version="true"></script>

<script>


    $("#LastModifiedAt").attr("value", function () {
        return moment($("#LastModifiedAt").val()).format("ddd, D/M/YYYY, h:mm a");
    });

    $("#_Expires").attr("value", function () {
        return moment($("#_Expires").val()).format("ddd, D/M/YYYY, h:mm a");
    });

    $("#_Revoked").attr("value", function () {
        return moment($("#_Revoked").val()).format("ddd, D/M/YYYY");
    });

    $("#LicenseDate").attr("value", function () {
        return moment($("#LicenseDate").val()).format("ddd, D/M/YYYY");
    });

    $("#_Date").attr("value", function () {
        return moment($("#_Date").val()).format("ddd, D/M/YYYY");
    });

    $("#DateOfBirth").attr("value", function () {
        return moment($("#DateOfBirth").val()).format("ddd, D/M/YYYY");
    });

    
    
</script>