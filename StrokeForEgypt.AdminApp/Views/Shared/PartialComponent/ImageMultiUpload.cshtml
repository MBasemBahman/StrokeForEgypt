@model MultiImageViewModel
<div class="row" id="Attachments">
    <div class="col">
        <section class="card">
            <header class="card-header">
                <div class="card-actions">
                    <a href="#" class="card-action card-action-toggle" data-card-toggle></a>
                    <a href="#" class="card-action card-action-dismiss" data-card-dismiss></a>
                </div>

                <h2 class="card-title">@Model.Name Upload Drag'n Drop</h2>
            </header>
            <div class="card-body">
                <form asp-action="@Model.UploudAction" enctype="multipart/form-data" class="dropzone dz-square" id="dropzone-example">
                    <input type="hidden" asp-for="@Model.Id" />
                </form>
            </div>
        </section>
    </div>
</div>
